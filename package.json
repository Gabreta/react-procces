{
  "name": "proy-1",
  "version": "1.0.0",
  "description": "Primer proyecto para webpack construccion",
  "scripts": {
    "build": "npm run clean && npm build-client && npm run build-server",
    "build-clean": "NODE_ENV=production BABEL_ENV=production node -r babel-register node_modules/.bin/webpack --progress --config webpack/webpack.config.server.js",
    "clean": "rimraf dist/ && rimraf public/app",
    "lint": "eslint src",
    "postmerge": "npm install",
    "postwrite": "npm install",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "start": "npm run clean & NODE_ENV=development BABEl_ENV=development babel-node src/server",
    "start-analyzer": "npm run clean && NODE_ENV=development BABEl_ENV=development ANALYZER=true babel-node src/server",
    "start-production": "npm run clean && NODE_ENV=production babel-node src/server",
    "test": "echo \"Error: no test",
    "watch-client": "webpack --watch --config webpack/webpack.config.client.js",
    "watch-server": "webpack --watch --config webpack/webpack.config.server.js"
  },
  "author": "Gabriel Vargas",
  "license": "GPL-3.0",
  "dependencies": {
    "express": "^4.16.2",
    "history": "^4.7.2",
    "html-webpack-plugin": "^2.30.1",
    "node-time-ago": "^1.0.0",
    "path": "^0.12.7",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "compression-webpack-plugin": "^1.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
